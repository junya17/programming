     <%= form_for(@programming, url:{controller:'programming', action:'update', id:@programming.id}) do |form|%>
            <div class="form-group">
                <tr>
                    <th>タイトル</th>
                    <td>
                        <%= form.text_field :title , size: 100, :class=>'form-control'%>
                    </td>
                </tr>
            </div>
            <div class="form-group">
                <tr>
                    <th>エラー</th>
                    <td>
                        <%= form.text_field :question, size: 100, :class=>'form-control' %>
                    </td>
                </tr>
            </div>
            <div class="form-group">
                <tr>
                    <th>解決策</th>
                    <td>
                        <%= form.text_field :answer, size: 100, :class=>'form-control' %>
                    </td>
                </tr>
            </div>
            <div class="form-group">
                <tr>
                    <th>参照リンク</th>
                    <td>
                        <%= form.text_field :url , size: 100, :class=>'form-control' %>
                    </td>
                </tr>
            </div>
            <div>
                <tr>
                    <th></th>
                    <td>
                        <%= form.submit "送信" , size: 100, :class=>"btn btn-primary"%>
                </tr>
            </div>
            <%end%>
